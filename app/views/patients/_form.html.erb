<%= simple_form_for(@patient) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :phone %>
    <%= f.input :birthday %>
    <%= f.input :cep %>
    <%= f.input :adress %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
