<%= simple_form_for(@anamnesi) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :queixa_principal %>
    <%= f.input :como_aconteceu %>
    <%= f.input :data_inicio %>
    <%= f.association :patient %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
