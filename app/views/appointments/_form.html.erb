<%= simple_form_for(@appointment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :comentario %>
    <%= f.input :data_entrada %>
    <%= f.input :data_saida %>
    <%= f.association :patient %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
